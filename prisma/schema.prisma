datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider             = "prisma-client-py"
  recursive_type_depth = 5
}

model Writing {
  id          String @id @default(cuid())
  title       String
  description String

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
